{
  "ipcMode": "host",
  "executionRoleArn": "arn:aws:iam::015838347042:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "dnsSearchDomains": [],
      "environmentFiles": [
        {
          "value": "/var/task/env"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "inn-dev-ecs-logs1",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs-task-logs-service1"
        }
      },
      "entryPoint": [
        "/usr/local/bin/docker-entrypoint.sh"
      ],
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "command": [
        "nginx",
        "-g",
        "daemon off;"
      ],
      "cpu": 10,
      "environment": [
        {
          "name": "ENV_VAR_1",
          "value": "value1"
        },
        {
          "name": "ENV_VAR_2",
          "value": "value2"
        }
      ],
      "memory": 300,
      "image": "015838347042.dkr.ecr.us-east-1.amazonaws.com/cicd-deplymt:127503e",
      "essential": true,
      "name": "nginx-container"
    }
  ],
  "placementConstraints": [],
  "taskRoleArn": "arn:aws:iam::015838347042:role/ecsTaskExecutionRole",
  "compatibilities": [
    "EC2"
  ],
  "family": "nginx-task-definition",
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "bridge",
  "cpu": "512",
  "revision": 1
}
